package com.krakedev;

public class TestAuto {

	public static void main(String[] args) {
		
		Auto auto1= new Auto();
		Auto auto2= new Auto();
		
		auto1.marca="Toyota";
		auto1.anio=2000;
		auto1.precio=18500.00;
		auto2.marca="Kia";
		auto2.anio=2011;
		auto2.precio=15000.00;
		
		System.out.println("auto.marca"+auto1.marca);
		System.out.println("auto.anio"+auto1.anio);
		System.out.println("auto.precio"+auto1.precio);
		System.out.println("*********************************");
		System.out.println("auto.marca"+auto2.marca);
		System.out.println("auto.anio"+auto2.anio);
		System.out.println("auto.precio"+auto2.precio);
	}

}
